        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#a3a300",
        "statusBar.background": "#707000",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#a3a300",
        "statusBarItem.remoteBackground": "#707000",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#707000",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#70700099",
        "titleBar.inactiveForeground": "#e7e7e799"
