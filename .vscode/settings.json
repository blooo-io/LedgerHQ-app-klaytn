{
    "files.associations": {
        "*.h": "c"
    },
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "editor.formatOnSave": false,
    "python.terminal.activateEnvironment": false,
    "buid_dir_relative_path": ".",
